+        /** 绘制横线*/
+        for (int i = 0; i < gridHorizontalNum + 1; i++) {
+            canvas.drawLine(0, i * GRID_WIDTH,
+                    mWidth, i * GRID_WIDTH, mLinePaint);
+        }
+        /** 绘制竖线*/
+        for (int i = 0; i < gridVerticalNum + 1; i++) {
+            canvas.drawLine(i * GRID_WIDTH, 0,
+                    i * GRID_WIDTH, mHeight, mLinePaint);
+        }
