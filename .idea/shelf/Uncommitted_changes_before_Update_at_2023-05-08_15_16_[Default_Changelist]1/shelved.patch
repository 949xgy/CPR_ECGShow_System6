+                    dataValue = MAX_VALUE;
+                }
+            } else {
+                if (dataValue < -MAX_VALUE) {
+                    dataValue = -MAX_VALUE;
+                }
+            }
+            nowY = mHeight / 2 - dataValue * (mHeight / (MAX_VALUE * 2));
